{
  "analyzeCommits": {
    "preset": "angular",
    "releaseRules": [
      { "type": "fix", "release": "patch" },
      { "type": "Fix", "release": "patch" },
      { "type": "Feature", "release": "minor" },
      { "type": "feature", "release": "minor" },
      { "type": "breaking", "release": "major" },
      { "type": "Breaking", "release": "major" }
    ]
  },
  "generateNotes": {
    "preset": "angular",
    "parserOpts": {
      "noteKeywords": [
        "fix",
        "Fix",
        "Feature",
        "feature",
        "breaking",
        "Breaking"
      ]
    },
    "writerOpts": {
      "commitsSort": "type",
      "transform": {}
    }
  }
}
